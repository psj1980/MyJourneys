{
  "name": "myjourneys",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "concurrently \"bun run dev\" \"bun run start:api\"",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start:api": "json-server --watch db.json --port 3001 --delay 1000",
    "storybook": "storybook dev -p 6006",
